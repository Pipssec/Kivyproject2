<Menu>:
    FloatLayout:
        AsyncImage:
            source: 'data/logo3.jpg'
    FloatLayout:
        Button:
            text: "2:1"
            pos_hint: {'x': .10, 'y': .75}
            size_hint: .2, .1
            font_size: 100
            on_press: root.manager.current = '2k1'

        Button:
            text: "1,5:1"
            pos_hint: {'x': .70, 'y': .75}
            size_hint: .2, .1
            font_size: 100
            on_press: root.manager.current = '15k1'

        Label:
            text: "Сахарный сироп(Cахар : Вода)"
            pos_hint: {'x': .0, 'y': .9}
            size_hint: 1, .1
            font_size: 70

        Button:
            text: "1:1"
            pos_hint: {'x': .70, 'y': .55}
            size_hint: .2, .1
            font_size: 100
            on_press: root.manager.current = '1k1'

        Button:
            text: "1:1,5"
            pos_hint: {'x': .70, 'y': .35}
            size_hint: .2, .1
            font_size: 100
            on_press: root.manager.current = '1k15'

        Button:
            text: "1:2"
            pos_hint: {'x': .70, 'y': .15}
            size_hint: .2, .1
            font_size: 100
            on_press: root.manager.current = '1k2'

        Button:
            text: "1:2,5"
            pos_hint: {'x': .10, 'y': .15}
            size_hint: .2, .1
            font_size: 100
            on_press: root.manager.current = '1k25'

        Button:
            text: "1:3"
            pos_hint: {'x': .10, 'y': .35}
            size_hint: .2, .1
            font_size: 100
            on_press: root.manager.current = '1k3'

        Button:
            text: "1:3,5"
            pos_hint: {'x': .10, 'y': .55}
            size_hint: .2, .1
            font_size: 100
            on_press: root.manager.current = '1k35'
