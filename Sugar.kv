<K1K1>:
    FloatLayout:
        AsyncImage:
            source: 'data/logo3.jpg'
    BoxLayout:
        orientation: 'vertical'
        padding: [30, 20, 30, 20]
        Label:
            text: "Введите количество сиропа в мл(1:1):"
            size_hint: 1, .05
            font_size: 50
        TextInput:
            id: sirop
            multiline: False
            size_hint: 1, .05
            input_type: 'number'
            input_filter: 'int'
        Label:
            size_hint: 1, .05
        Button:
            text: "Рассчитать"
            size_hint: 1, .1
            font_size: 50
            on_release: root.rasschet()
        Label:
            text: root.sugar
            size_hint: 1, .4
            font_size: 100
        Label:
            text: root.water
            size_hint: 1, .4
            font_size: 100
        Button:
            text: "Меню"
            size_hint: 1, .1
            font_size: 50
            on_release: root.manager.current = 'menu'

